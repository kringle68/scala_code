	font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
	font-weight: bold;        
	border:0px;
	background-color: #333;
}

#subnav h3	{
	margin: 0;
	padding: 0px 5px 0px 0;
	font-weight: bold; 
	font-size: 1em;
	line-height: 2.0em;     
    text-decoration: none;    
	color: #666666;
	width: 119px;
	border-bottom: 1px dashed #bbb;
}

#subnav input { 
	color: #333;
	font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
	font-weight: normal;        
  	font-size: 1em;
	margin-top:3px;
}

#subnav input.button { 
	padding-left: 4px;
	padding-right: 2px;
	padding-top: 1px;
	padding-bottom: 1px;
	color: #fff;
	font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
	font-weight: bold;        
  	font-size: 1em;
	border:0px;
	background-color: #333;
	margin-bottom:1px;
	margin-top:3px;
}

#subnav a	{
	margin: 0;
	padding: 3px 0 3px 0;
	line-height: 2.0em;     
    text-decoration: none;    
	color: #CC6600;
	width: 119px;
	border-bottom: 1px dashed #bbb;
}

#handheldSubnav a	{
	margin: 0;
	padding: 3px 0 3px 0;
	line-height: 2.0em;     
    text-decoration: none;    
	color: #CC6600;
	border-bottom: 0px;
}	

#subnav a:hover	{   
    text-decoration: none;    
	color: #999999;
}

#subnav a.first	{
	border-top: 1px dashed #bbb;
	padding-top:3px;
	margin-top:-5px;
}

#subnav a.last	{
	border-bottom: 0;
}

/*  Horizontal nav */

#menu {
background-color: transparent; 
z-index:10000;
float: left;
visibility:hidden;
}


#menu div.top {
	border: 0;
	color: #000;
	z-index:10000;
    width: 120px;
	height: 25px;
	padding:0;
	margin:0;
	margin-right:4px;
	margin-top:1px;
	margin-bottom:1px;
}

#menu div.section {
    position: absolute;
    width: 92px;
	z-index:10000;
    display: inline;
    z-index: -1; 
}

#menu div.box {
    background-color: #CC6600;
    border-color: #ffffff;
    border-width: 1px;
	left:-1px;
	/*text-align:center;*/
    border-style: none solid solid solid;
    position: relative;
    visibility: hidden;
	z-index:10000;
    width: 100%; 
}

#menu div.emptytop {
	border: 0;
	color: #000;
	z-index:10000;
    width: 123px;
	height: 26px;
	padding:0;
	margin:0;
	margin-right:0px;
	margin-top:1px;
	margin-bottom:1px;
	border-bottom: 5px solid #000;
	font: bold 14px Arial, Helvetica, sans-serif;
}

