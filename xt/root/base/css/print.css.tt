        font-size: 1.0em;
        }

.warning      {
        display: inline;
		line-height: 1.7em;
        padding: 5px;
        background-color: #993300;
        color: #ffffff;
        text-transform: none;
        font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
        font-weight: bold;
        font-size: 1.0em;
        }

#content .title {
    display: inline;
    line-height: 2.2em;
    padding: 0;
    background: transparent;
    color: #777;
    text-transform: none;
    font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
    font-weight: bold;
    font-size: 1.1em;
}


#maincontent  {
	color: #333;
	font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
	font-weight: normal;        
  	font-size: 0.7em;
}

#handheld_maincontent  {
	color: #333;
	font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
	font-weight: normal;        
  	font-size: 0.5em;
}

#footer  {
	color: #fff;
	font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
	font-weight: normal;        
  	font-size: 0.6em;
}

#maincontent a	{   
    text-decoration: underline;    
	color: #333;
}	

#maincontent a.zoom	{   
    text-decoration: none;
}

#content table td {
    color: #333;
    font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
    font-weight: normal;
    font-size: 0.7em;
}

#content table td.divider {
    background-image: url("/images/dots.gif");
    height:1px;
}

#content table.data tr {
    color: #333;
    font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
    font-weight: normal;
    background-color: #f2f2f2;
}

#content table.data td {
    height:24px;
}

#content table.data td.light {
    color: #999;
}
#content table.data td.light a {
    color: #999;
    text-decoration: underline;
}

#content table.data td.light a:hover {
    text-decoration: none;
}

#content table.data td.light .highlight {
    color: #999;
}

#content table.data td.divider {
    background-image: url("/images/dots.gif");
    height:1px;
}

#content table.data td.dividerHeader {
    background-image: url("/images/dots.gif");
    background-color:#e5e5e5;
    height:1px;
}

#content table.data td.tableHeader {
    color: #333;
    font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
    font-weight: bold;
    background-color: #dddddd;
    height:24px;
}

#content table.data td.tableHeader .highlight {
    color: #000;
    font-weight: bold;
}

#content table.data td.blank {
    background-color:#FFF;
    height:1px;
}

#content table.data td.spacer {
         background-color:#FFF;
         height:20px;
}

#content table.data td.none {
         background-color:#FFF;
         height:0px;
}

#content table.data td.warn {
         background-color:#F0B9B9;
}

#content table.data td.highlight {
         background-color: #CCFFCC;
}

#content table.data tr.hoverEffect {
    background-color: [% NAP.secondary_colour %];
}

#maincontent input { 
	color: #333;
	font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
	font-weight: normal;      
	font-size: 1em;  
}

#maincontent select {
        color: #333;
        font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
