    padding: 0;
    background: transparent;
    color: #777;
    text-transform: none;
    font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
    font-weight: bold;
    font-size: 1.4em;
}

h2  {
    margin: 0 10px 0 0;
    display: inline;
    padding: 0;
    background: transparent;
    color: #777;
    text-transform: none;
    font-family: Arial, Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
    font-weight: bold;
    font-size: 1.2em;
}

h3  {
    margin: 0 10px 0 0;
    display: inline;
    padding: 0;
    background: transparent;
