<span class="title title-[% channel_config.$sales_channel %]">Returns</span><br>

[% IF returns.size > 0 %]
<table width="100%" cellpadding="0" cellspacing="0" border="0" class="data">

	[% FOREACH returnid = returns.keys %]

	<tr>
		<td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr>
		<td align="right" width="15%"><b>RMA:&nbsp;</td>
		<td><a href="[% short_url %]/Returns/View?order_id=[% order_id %]&shipment_id=[% shipment_id %]&return_id=[% returnid %]">[% returns.$returnid.rma_number %]</a></td>
		<td><img src="/images/blank.gif" width="1" height="24"></td>
	</tr>
	<tr>
		<td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr height="24">
		<td align="right" class="blank"><b>Date Created:&nbsp;</td>
		<td class="blank">[% returns.$returnid.date_created %]</td>
		<td class="blank"><img src="/images/blank.gif" width="1" height="24"></td>
	</tr>
	<tr>
		<td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr height="24">
		<td align="right" class="blank"><b>Status:&nbsp;</td>
		<td class="blank"><span class="highlight">[% returns.$returnid.status %]</span></td>
		<td class="blank"><img src="/images/blank.gif" width="1" height="24"></td>
	</tr>
	<tr>
		<td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr height="24">
		<td align="right" class="blank"><b>Notes:&nbsp;</td>
		<td class="blank">[% returns.$returnid.comment %]</td>
		<td class="blank"><img src="/images/blank.gif" width="1" height="24"></td>
	</tr>
	<tr>
		<td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr>
		<td colspan="3" class="blank"><img src="/images/blank.gif" width="1" height="15"></td>
	</tr>
	<tr>
		<td colspan="3">
			<table width="100%" cellpadding="0" cellspacing="0" border="0" class="data">
				<tr>
					<td colspan="7" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
				</tr>
				<tr>
                    <td class="tableHeader">&nbsp;</td>
                    <td class="tableHeader">Product</td>
                    <td class="tableHeader">Size</td>
                    <td class="tableHeader">Type</td>
                    <td class="tableHeader">Reason</td>
                    <td class="tableHeader">Status</td>
                    <td class="tableHeader">Date Received</td>
                </tr>
				<tr>
					<td colspan="7" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
				</tr>

                [% FOREACH returnitemid = returns.$returnid.return_items.keys %]	
                    [% SET shipitemid = returns.$returnid.return_items.$returnitemid.shipment_item_id %]
                    [% SET style = '' %]
                    [% IF returns.$returnid.return_items.$returnitemid.status == 'Cancelled' %]
                        [% SET style = '<span class="lowlight">' %]
                    [% END %]
                    <tr>
                        <td align="center"><a href="[% returns.$returnid.return_items.$returnitemid.images.0 %]" title="View Product Image" target="new" class="imagezoom"><img src="/images/icons/image.png"></a></td>
                        <td>[% style %][% returns.$returnid.return_items.$returnitemid.sku %]</td>
                        <td>[% style %][% IF shipment_items.$shipitemid.short_name %][% shipment_items.$shipitemid.short_name %] [% END %][% shipment_items.$shipitemid.designer_size %][% IF shipment_items.$shipitemid.designer_size != shipment_items.$shipitemid.size %] ([% shipment_items.$shipitemid.size %])[% END %]</td>
                        <td>[% style %][% returns.$returnid.return_items.$returnitemid.type %]</td>
                        <td>[% style %][% returns.$returnid.return_items.$returnitemid.reason %]</td>
                        <td>[% style %][% returns.$returnid.return_items.$returnitemid.status %]</td>
                        <td>[% style %][% returns.$returnid.return_items.$returnitemid.date_received %]</td>
                    </tr>        
                    <tr>
                        <td>&nbsp;</td>
                        <td colspan="6" valign="top">[% style %]<i>[% shipment_items.$shipitemid.designer %] - [% shipment_items.$shipitemid.name %]</i></td>
                    </tr>
                    <tr>
                        <td colspan="7" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
                    </tr>
                [% END %]
			</table>
		
		</td>
	</tr>
	<tr>
		<td colspan="3" class="blank"><img src="/images/blank.gif" width="1" height="35"></td>
	</tr>

	[% END %]	
</table>

<br><br><br>

[% ELSE %]

    No returns for this shipment.

[% END %]

[% ELSIF shipments.size > 0 %]

<span class="title title-[% channel_config.$sales_channel %]">Shipments</span><br>

<table width="100%" cellpadding="0" cellspacing="0" border="0" class="data">
	<tr>
		<td colspan="9" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr height="24">
		<td class="tableHeader">&nbsp;&nbsp;&nbsp;Shipment Number</td>
		<td class="tableHeader">Date</td>
		<td class="tableHeader">Type</td>
		<td class="tableHeader">Status</td>
	</tr>
	<tr>
		<td colspan="9" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	
[% FOREACH id = shipments.keys %]
	<tr>
		[% IF shipments.$id.shipment_status_id == 4 %]
			<td>&nbsp;&nbsp;&nbsp;<a href="[% short_url %]/Returns/View?order_id=[% order_id %]&shipment_id=[% id %]">[% id %]</a></td>
			<td>[% shipments.$id.date %]</td>
			<td>[% shipments.$id.class %]</td>
			<td>[% shipments.$id.status %]</td>
		[% ELSE %]
			<td class="light">&nbsp;&nbsp;&nbsp;[% id %]</td>
			<td class="light">[% shipments.$id.date %]</td>
			<td class="light">[% shipments.$id.class %]</td>
			<td class="light">[% shipments.$id.status %]</td>
		[% END %]		
	</tr>
	<tr>
		<td colspan="4" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
[% END %]

</table>

[% ELSE %]

nothing found!

[% END %]

<br><br><br><br>
