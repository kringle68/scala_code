
[% IF return_id != "" %]

<h3 class="title-[% channel_config.$sales_channel %]">Return Details</h3>

<table width="100%" cellpadding="0" cellspacing="0" border="0" class="data" id="return_details">
	<tr>
		<td colspan="2" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr>
		<td align="right" width="15%"><strong>RMA:</strong>&nbsp;&nbsp;</td>
		<td>[% return.rma_number %]</td>
	</tr>
	<tr>
		<td colspan="2" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr height="24">
		<td align="right" class="blank"><strong>Date Created:</strong>&nbsp;&nbsp;</td>
		<td class="blank">[% return.date_created %]</td>
	</tr>
	<tr>
		<td colspan="2" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr height="24">
		<td align="right" class="blank"><strong>Expiry Date:</strong>&nbsp;&nbsp;</td>
		<td class="blank">[% return.expiry_date %]</td>
	</tr>
	<tr>
		<td colspan="2" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr height="24">
		<td align="right" class="blank"><strong>Cancellation Date:</strong>&nbsp;&nbsp;</td>
		<td class="blank">[% return.cancellation_date %]</td>
	</tr>
	<tr>
		<td colspan="2" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr height="24">
		<td align="right" class="blank"><strong>Status:</strong>&nbsp;&nbsp;</td>
		<td class="blank"><span class="highlight">[% return.status %]</span></td>
	</tr>
	<tr>
		<td colspan="2" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr>
		<td colspan="2" class="blank"><img src="/images/blank.gif" width="1" height="25"></td>
	</tr>
</table>

[% IF return_notes.size > 0 %]

	<h3 class="title-[% channel_config.$sales_channel %]">Return Notes</h3>
	<table width="100%" cellpadding="0" cellspacing="0" border="0" class="data" id="return_notes">
		<tr>
			<td colspan="6" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
		</tr>
		<tr>
			<td class="tableHeader" width="17%">&nbsp;&nbsp;&nbsp;Date</td>
			<td class="tableHeader" width="53%">Note</td>
			<td class="tableHeader" width="10%">Type</td>
			<td class="tableHeader">&nbsp;&nbsp;&nbsp;&nbsp;</td>
			<td class="tableHeader" width="12%">Operator</td>
			<td class="tableHeader" width="8%">Dept.</td>
		</tr>
		<tr>
			<td colspan="6" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
		</tr>
	[% FOREACH ret_note_id = return_notes.keys.nsort %]	
		<tr>
			<td>&nbsp;&nbsp;&nbsp;[% return_notes.$ret_note_id.date %]</td>
			<td>[% return_notes.$ret_note_id.note | html | html_line_break %]</td>
			<td>[% return_notes.$ret_note_id.description %]</td>
			<td>&nbsp;&nbsp;&nbsp;&nbsp;</td>
			<td>[% return_notes.$ret_note_id.name %]</td>
			<td>[% return_notes.$ret_note_id.department %]</td>
		</tr>
		<tr>
			<td colspan="6" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
		</tr>
	[% END %]
		<tr>
			<td colspan="6" class="blank"><img src="/images/blank.gif" width="1" height="20"></td>
		</tr>
	</table>
[% END %]
	
<h3 class="title-[% channel_config.$sales_channel %]">Return Items</h3>
<table width="100%" cellpadding="0" cellspacing="0" border="0" class="data" id="return_items">
	<tr>
		<td colspan="7" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr>
		<td class="tableHeader">&nbsp;</td>
        <td class="tableHeader">Product</td>
        <td class="tableHeader">Size</td>
		<td class="tableHeader">Type</td>
		<td class="tableHeader">Reason</td>
		<td class="tableHeader">Status</td>
		<td class="tableHeader">Date Received</td>
	</tr>
	<tr>
		<td colspan="7" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>

	[% FOREACH ri = return_items %]
        [% SET shipitemid = ri.shipment_item_id %]
        [% SET style = '' %]
        [% IF ri.status == 'Cancelled' %]
            [% SET style = '<span class="lowlight">' %]
        [% END %]
        <tr>
            <td align="center"><a href="[% ri.images.0 %]" title="View Product Image" target="new" class="imagezoom"><img src="/images/icons/image.png"></a></td>
            <td>[% style %][% ri.sku %]</td>
            <td>[% style %][% IF shipment_items.$shipitemid.short_name %][% shipment_items.$shipitemid.short_name %] [% END %][% shipment_items.$shipitemid.designer_size %][% IF shipment_items.$shipitemid.designer_size != shipment_items.$shipitemid.size %] ([% shipment_items.$shipitemid.size %])[% END %]</td>
            <td>[% style %][% ri.type %]</td>
            <td>[% style %][% ri.reason %]</td>
            <td>[% style %][% ri.status %]</td>
            <td>[% style %][% ri.date_received %]</td>
        </tr>
        <tr>
            <td>&nbsp;</td>
            <td colspan="6" valign="top">[% style %]<i>[% shipment_items.$shipitemid.designer %] - [% shipment_items.$shipitemid.name %]</i></td>
        </tr>
        <tr>
            <td colspan="7" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
        </tr>
	[% END %]
</table>

<br><br><br>

<h3 class="title-[% channel_config.$sales_channel %]">Return Log</h3>

<table width="100%" cellpadding="0" cellspacing="0" border="0" class="data" id="return_log">
	<tr>
		<td colspan="3" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr>
		<td class="tableHeader">&nbsp;&nbsp;&nbsp;Date</td>
		<td class="tableHeader">&nbsp;&nbsp;&nbsp;Status</td>
		<td class="tableHeader">&nbsp;&nbsp;&nbsp;Operator</td>
	</tr>
	<tr>
		<td colspan="3" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>

	[% FOREACH log_id = return_log.keys.nsort %]
	
	<tr>
		<td>&nbsp;&nbsp;&nbsp;[% return_log.$log_id.date %]</td>
		<td>&nbsp;&nbsp;&nbsp;[% return_log.$log_id.status %]</td>
		<td>&nbsp;&nbsp;&nbsp;[% return_log.$log_id.name %]</td>
	</tr>
	<tr>
		<td colspan="3" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>


	[% END %]
</table>

<br><br><br>

<h3 class="title-[% channel_config.$sales_channel %]">Return Items Log</h3>

<table width="100%" cellpadding="0" cellspacing="0" border="0" class="data" id="return_items_log">
	<tr>
		<td colspan="4" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>
	<tr>
		<td class="tableHeader">&nbsp;&nbsp;&nbsp;Date</td>
		<td class="tableHeader">&nbsp;&nbsp;&nbsp;SKU</td>
		<td class="tableHeader">&nbsp;&nbsp;&nbsp;Status</td>
		<td class="tableHeader">&nbsp;&nbsp;&nbsp;Operator</td>
	</tr>
	<tr>
		<td colspan="4" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
	</tr>

	[% FOREACH item_log_id = return_items_log.keys.nsort %]
	
		[% SET ri_id = return_items_log.$item_log_id.return_item_id %]
	
		<tr>
			<td>&nbsp;&nbsp;&nbsp;[% return_items_log.$item_log_id.date %]</td>
			<td>&nbsp;&nbsp;&nbsp;[% return_items_sku.$ri_id %]</td>
			<td>&nbsp;&nbsp;&nbsp;[% return_items_log.$item_log_id.status %]</td>
			<td>&nbsp;&nbsp;&nbsp;[% return_items_log.$item_log_id.name %]</td>
		</tr>
		<tr>
			<td colspan="4" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
		</tr>


	[% END %]
</table>

<br><br><br>

[% IF return_email_log.size %]
    [% PROCESS page_elements/display_return_email_logs.tt ret_tab_id='return_email_log',ret_email_logs=return_email_log %]
    <br/><br/><br/>
[% END %]

