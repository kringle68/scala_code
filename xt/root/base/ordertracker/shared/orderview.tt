                        <td colspan="7" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
                    </tr>
                    <tr height="24">
                        <td class="tableHeader" width="12%">&nbsp;&nbsp;&nbsp;SKU</td>
                        <td class="tableHeader" width="27%">Name</td>
                        <td class="tableHeader" width="23%">Designer</td>
                        <td class="tableHeader" width="13%">Size</td>
                        <td class="tableHeader" width="15%">Customs Value</td>
                        <td class="tableHeader" width="10%">Unit Weight</td>
                    </tr>
                    <tr>
                        <td colspan="7" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
                    </tr>
                    [% FOREACH itemid = shipments.$id.ship_items.keys %]
