                        <td[% tdclass %]><a href="[% short_url %]/Invoice?order_id=[% order.id %]&shipment_id=[% renumeration.shipment_id %]&action=View&invoice_id=[% renumeration.id %]"><img src="/images/icons/page_go.png" alt="View Invoice"></a></td>
                        <td[% tdclass %]>
                            [% IF renumeration.renumeration_status_id < 4 %]
                                <a href="[% short_url %]/Invoice?order_id=[% order.id %]&shipment_id=[% renumeration.shipment_id %]&action=Edit&invoice_id=[% renumeration.id %]"><img src="/images/icons/page_edit.png" alt="Edit Invoice"></a>
                            [% END %]
                        </td>
                    </tr>
                    <tr>
                        <td colspan="7" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                    </tr>
                [% END %]
            [% END %]
        </table>

        <br /><br /><br />

    [% END %]


    [% IF shipments.$id.returns.size > 0 %]

        [% FOREACH returnid = shipments.$id.returns.keys %]
