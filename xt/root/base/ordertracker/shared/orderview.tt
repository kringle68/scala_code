                <thead>
                    <tr>
                        <th>SKU</th>
                        <th>Type</th>
                        <th>Reason</th>
                        <th>Status</th>
                        <th>Date Received</th>
                        <th>AWB</th>
                    </tr>
                </thead>
                <tbody>
