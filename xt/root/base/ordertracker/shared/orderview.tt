                    <td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                </tr>
                <tr height="24">
                    <td align="right" class="blank"><b>Date Created:</b>&nbsp;</td>
                    <td class="blank">[% shipments.$id.returns.$returnid.date_created %]</td>
                    <td class="blank"><img src="/images/blank.gif" width="1" height="24"></td>
                </tr>
                <tr>
                    <td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                </tr>
                <tr height="24">
                    <td align="right" class="blank"><b>Status:</b>&nbsp;</td>
                    <td class="blank"><span class="highlight">[% shipments.$id.returns.$returnid.status %]</span></td>
