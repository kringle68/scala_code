                    <td[% class %] align="right">[% sitem.tax %]</td>
                    <td[% class %] align="right">[% sitem.duty %]</td>
                    <td[% class %] align="right">[% sitem.sub_total %]&nbsp;&nbsp;</td>
                </tr>
            [% END %]

            [%- BLOCK summary_row %]
                <td class="blank" colspan="8">&nbsp;</td>
                <td class="blank" colspan="3" align="right"><b>[% row_name %]</b>&nbsp;</td>
                <td class="blank" align="right" style="white-space:nowrap;">[% '<span class="lowlight">' IF is_cancelled %]<b>[% row_value %]&nbsp;&nbsp;</b>[% '</span>' IF is_cancelled %]</td>
