            [% SET shipments.$id.boxes.$boxid.total_weight = shipments.$id.boxes.$boxid.weight %]

            [% FOREACH itemid = shipments.$id.ship_items.keys %]

                [% IF shipments.$id.ship_items.$itemid.shipment_box_id == boxid %]
                    [% shipments.$id.boxes.$boxid.num_items = shipments.$id.boxes.$boxid.num_items + 1 %]
