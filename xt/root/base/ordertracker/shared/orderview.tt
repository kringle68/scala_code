    [% IF shipments.$id.paperwork.size > 0 %]
        <h3 class="title-[% channel_config.$sales_channel %]" id="print_log">Print Log</h3>
        <form name="reprintForm" action="[% url %]" method="POST">
            [% INCLUDE page_elements/forms/dbl_submit_token.tt %]
            <table class="data wide-data divided-data">
                <thead>
                    <tr>
                        <th>Document</th>
                        <th>Date Printed</th>
                        <th>Printer</th>
                        <th style="width: 30%">Re-Print</th>
                    </tr>
                </thead>
                <tbody>
                    [% FOREACH docid = shipments.$id.paperwork.keys.nsort %]
                        <tr>
                            [%# Intercept and deparse MRP Sticker rows and do something nice #%]
                            [% SET particles = shipments.$id.paperwork.$docid.document.match('MRP Sticker (\d+) cop') %]
                            [% IF particles.0 %]
                                <td>MrP Sticker ([% particles.0 %]): "[% shipments.$id.paperwork.$docid.file | html %]"</td>
                            [% ELSE %]
                                <td>[% '<a href="' _ url _ '&viewdoc=' _ docid _ '" target="_blank">' IF shipments.$id.paperwork.$docid.file; shipments.$id.paperwork.$docid.document; '</a>' IF shipments.$id.paperwork.$docid.file %]</td>
                            [% END %]
                            [% printer_name = shipments.$id.paperwork.$docid.name %]
                            <td>[% shipments.$id.paperwork.$docid.date %]</td>
                            <td>[% printer_name %]</td>
                            [% IF particles.0 %]
                                <td><i>See "Personalized Sticker" above for reprints</i></td>
                            [% ELSE %]
                                <td>[% IF printer_name != '-' %]<input type="radio" name="reprint" value="[% shipments.$id.paperwork.$docid.file %]">[% END %]</td>
                            [% END %]
                        </tr>
                    [% END %]
                </tbody>
            </table>
            [% INCLUDE printer_selection %]
            <div class="formrow buttons aftertable">
                <input type="submit" class="button" name="submit" value="Re-Print Documents &raquo;">
            </div>
        </form>
    [% END %]

    [% IF shipments.$id.pick_paperwork.size > 0 %]
        <h3 class="title-[% channel_config.$sales_channel %]" id="pick_prints">Print at pick items</h3>
        <form name="pick_prints" action="[% url %]" method="POST">
            [% INCLUDE page_elements/forms/dbl_submit_token.tt %]
            <table class="data wide-data divided-data">
                <thead>
                    <tr>
                        <th>Document</th>
                        <th>Document contents</th>
                        <th style="width: 30%">Regenerate and Print</th>
                    </tr>
                </thead>
                <tbody>
                    [% FOREACH docid = shipments.$id.pick_paperwork.keys.sort %]
                        <tr>
                            <td>[% shipments.$id.pick_paperwork.$docid.fullname %]</td>
                            <td>[% shipments.$id.pick_paperwork.$docid.description.replace('\n', '<br>') %]</td>
                            [%# Intercept and deparse MRP Sticker rows and do something nice #%]
                            [% IF docid == 'MrPSticker' %]
                                <td><i>See "Personalized Sticker" above for reprints</i></td>
                            [% ELSE %]
                                <td><input type="radio" name="pick_print" value="[% docid %]"></td>
                            [% END %]
                        </tr>
                    [% END %]
                </tbody>
            </table>
            [% INCLUDE printer_selection %]
            <div class="formrow buttons aftertable">
                <input type="hidden" name="shipment_id" value="[% id %]">
                <input type="submit" class="button" name="submit" value="Regenerate and Print Documents &raquo;">
            </div>
        </form>
    [% END %]


    [% BLOCK printer_selection %]
        <div class="formrow aftertable">
            <label for="select_printer">Select printer :</label>
            <select name="printer" id="select_printer">
                <option value="0">.....</option>
                [% FOREACH printer = printers %]
                    <option value="[% printer %]">[% printer %]</option>
                [% END %]
            </select>
        </div>
    [% END %]


[% END %]

[% BLOCK show_routing_schedule %]
<table class="wide-data data" id="rout_sched_[% rout_sched_id %]">
    <thead>
        <tr>
            <th>Date</th>
            <th>Task Window</th>
            <th>Driver</th>
            <th>Run Number</th>
            <th>Outcome</th>
            <th width="20%">Signatory</th>
            <th>Undelivered Notes</th>
        </tr>
    </thead>
    <tbody>
        [% IF schedules %]
            [% FOREACH schedule IN schedules %]
            [%-
                SET class = "";
                IF base_obj.is_cancelled;
                    class = " class='light'";
                ELSIF ( schedule.success );
                THEN;
                    class   = " class='highlight2'";
                END;
            -%]
            <tr class="dividebelow">
                <td[% class %]>[% IF schedule.task_window_date %][% order.channel.business.branded_date( schedule.task_window_date ) %][% END %]</td>
                <td[% class %]>[% schedule.task_window %]</td>
                [% IF schedule.task_window != 'TBC' %]
                <td[% class %]>[% schedule.driver | html %]</td>
                <td[% class %]>[% schedule.run_number %] [% IF schedule.run_order_number %]<em title="Run Order Number">([% schedule.run_order_number %])</em>[% END %]</td>
                [% ELSE %]
                <td[% class %]>&nbsp;</td>
                <td[% class %]>&nbsp;</td>
                [% END %]
                <td[% class %]>[%-
                        SET outcome = 'Pending';
                        IF schedule.success;
                            outcome = 'Success';
                        END;
                        IF schedule.failed;
                            outcome = 'Failed';
                        END;
                        IF schedule.resched;
                            outcome = 'Re-Scheduled';
                        END;
                    -%]
                    [%- outcome -%]
                </td>
                [% IF schedule.success %]
                <td[% class %]>[% IF schedule.signatory %]<strong>[% schedule.signatory | html %]</strong>[% IF schedule.signature_time %]<br/>[% order.channel.business.branded_date( schedule.signature_time ) %] @ [% schedule.signature_time.format_cldr("HH':'mm") %][% END %][% END %]</td>
                [% ELSE %]
                <td[% class %]>&nbsp;</td>
                [% END %]
                <td[% class %]>[% IF schedule.failed %][% schedule.undelivered_notes | html %][% END %]</td>
            </tr>
            [% END %]
        [% ELSE %]
            <tr class="dividebelow">
                <td[% class %] colspan="7">Premier delivery/collection details are currently not available, please refer to SATNAP for the most up to date information.</td>
            </tr>
        [% END %]
    </tbody>
</table>
<br/><br/>
[% END %]

[% BLOCK third_party_payment_message %]
    [%- IF order.is_paid_using_third_party_psp -%]
        [%- SET third_party_payment = order.get_third_party_payment_method -%]
        <div id="[% div_id %]" class="third_party_payment_message spaced">
            <strong>Please Note:</strong> this Order was paid with using <span>[% third_party_payment.payment_method %]</span>.
        </div>
    [%- END -%]
[% END %]

