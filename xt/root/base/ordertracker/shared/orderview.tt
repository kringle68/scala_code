                            <tr height="24">
                                <td>&nbsp;&nbsp;&nbsp;[% shipments.$id.ship_items.$itemid.product_id %]-[% shipments.$id.ship_items.$itemid.sku_size %]</td>
                                <td>[% shipments.$id.ship_items.$itemid.name %]</td>
                                <td>[% shipments.$id.ship_items.$itemid.designer %]</td>
                                <td>[% IF shipments.$id.ship_items.$itemid.short_name != "" %][% shipments.$id.ship_items.$itemid.short_name %] [% END %][% shipments.$id.ship_items.$itemid.designer_size %]</td>
                                <td>[% shipments.$id.ship_items.$itemid.customs_value %]&nbsp;</td>
                                <td>[% shipments.$id.ship_items.$itemid.weight | format ('%.3f') %]&nbsp;[% weight_unit %]</td>
                            </tr>
                            <tr>
                                <td colspan="9" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                            </tr>
                        [% END %]
                    [% END %]
                </table>
                <br><br>

                [% IF box_count != num_boxes %]
