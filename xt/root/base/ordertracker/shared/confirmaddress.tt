                                    [% FOREACH shipping_charge = new_shipping_option.shipping_charges.values.sort("description") %]
                                        <option value="[% shipping_charge.id %]"[% IF new_shipping_option.default_shipping_charge_id == shipping_charge.id %] selected[% END %]>[% shipping_charge.description %]</option>
                                    [% END %]
                                </select>
