                                        <td>[% shipment_items.$id.name %]</td>
                                        <td>[% shipment_items.$id.unit_price %]</td>
                                        <td>[% shipment_items.$id.tax %]</td>
                                        <td>[% shipment_items.$id.duty %]</td>
