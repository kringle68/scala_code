                                            <input type="hidden" size="5" name="diff_shipping" value="[% shipment_data.shipping_charge - new_shipping_charge %]">
                                            <span class="highlight">
                                        [% END %]

                                        [% new_shipping_charge %]
                                </td>
                                <td></td>
                                <td></td>
                                </tr>
                                <tr>
                                        <td colspan="9" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                                </tr>
                                <tr>
                                    <td class="blank"><img src="/images/blank.gif" width="1" height="24"></td>
                                    <td class="blank" colspan="7" align="right"><b>Current Total:</td>
                                    <td class="blank" align="right"><b>[% current_total %]&nbsp;[% order_data.currency %]</b></td>
                                </tr>
                                <tr>
                                    <td colspan="9" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                                </tr>
                                <tr>
                                    <td class="blank"><img src="/images/blank.gif" width="1" height="24"></td>
                                    <td class="blank" colspan="7" align="right"><b>New Total:</td>
                                    <td class="blank" align="right"><b>[% new_total %]&nbsp;[% order_data.currency %]</b></td>
                                </tr>
                                <tr>
                                        <td colspan="9" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                                </tr>
                                <tr>
                                    <td class="blank"><img src="/images/blank.gif" width="1" height="24"></td>
                                    <td class="blank" colspan="7" align="right"><b>Difference:</td>
                                    <td class="blank" align="right"><b>[% price_difference %]&nbsp;[% order_data.currency %]</b></td>
                                </tr>
                                <tr>
                                        <td colspan="9" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                                </tr>
                                [% IF payment == 1 %]

                                    <tr>
                                        <td class="blank" colspan="2"><img src="/images/blank.gif" width="1" height="24"></td>
                                        <td class="blank"></td>
                                        <td class="blank"></td>
                                        <td class="blank"></td>
                                        <td class="blank" align="right"><b>Type:</td>
                                        <td class="blank" colspan="3"><b>
                                            [% IF price_difference > 3 %]
                                                &nbsp;&nbsp;Card Debit <input type="hidden" name="refund_type" value="3">
                                            [% ELSIF price_difference < -3 %]
                                                &nbsp;&nbsp;<input type="radio" name="refund_type" value="2" checked>&nbsp;Card Refund&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="radio" name="refund_type" value="1" selected>&nbsp;Store Credit
                                            [% END %]
                                        </td>
                                    </tr>
                                    <tr>
                                            <td colspan="9" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                                    </tr>
                                [% ELSE %]
                                    [% IF price_difference != "0.00" %]
                                        <tr>
                                            <td class="blank" colspan="2"><img src="/images/blank.gif" width="1" height="24"></td>
                                            <td class="blank"></td>
                                            <td class="blank"></td>
                                            <td class="blank"></td>
                                            <td class="blank"></td>
                                            <td class="blank" colspan="3">
                                                This amount will be [% IF price_difference > 0 %]added to[% ELSE %]taken off[% END %] the payment which is taken once the shipment is packed.
                                            </td>
                                        </tr>
                                    [% END %]
                                [% END %]
                        </table>
                        <br><br>

                        [% IF payment == 0 || price_difference > 3 %]

                            <input type="hidden" name="email_content_type" value="[% email_content_type %]">

                            <span class="title title-[% channel_config.$sales_channel %]">Customer Email</span><br>

                            <table id="table__customer_email" width="100%" cellpadding="0" cellspacing="0" border="0" class="data">
                                <tr>
                                    <td colspan="2" class="divider"></td>
                                </tr>
                                <tr height="24">
                                    <td width="149" align="right"><b>Send Email:</b>&nbsp;&nbsp;</td>
                                    <td width="570"><input type="radio" name="send_email" value="yes" checked>&nbsp;&nbsp;Yes&nbsp;&nbsp;&nbsp;&nbsp;<input type="radio" name="send_email" value="no">&nbsp;&nbsp;No</td>
                                </tr>
                                <tr>
                                    <td colspan="2" class="divider"></td>
                                </tr>
                                <tr height="30">
                                    <td align="right"><b>To:</b>&nbsp;&nbsp;</td>
                                    <td><input type="text" size="35" name="email_to" value="[% order_data.email %]"></td>
                                </tr>
                                <tr>
                                    <td colspan="2" class="divider"></td>
                                </tr>
                                <tr height="30">
                                    <td align="right"><b>From:</b>&nbsp;&nbsp;</td>
                                    <td><input type="text" size="35" name="email_from" value="[% from_email %]"></td>
                                </tr>
                                <tr>
                                    <td colspan="2" class="divider"></td>
                                </tr>
                                <tr height="30">
                                    <td align="right"><b>Reply-To:</b>&nbsp;&nbsp;</td>
                                    <td><input type="text" size="35" name="email_replyto" value="[% from_email %]"></td>
                                </tr>
                                <tr>
                                    <td colspan="2" class="divider"></td>
                                </tr>
                                <tr height="30">
                                    <td align="right"><b>Subject:</b>&nbsp;&nbsp;</td>
                                    <td><input type="text" size="35" name="email_subject" value="[% email_subject %]"></td>
                                </tr>
                                <tr>
                                    <td colspan="2" class="divider"></td>
                                </tr>
                                <tr>
                                    <td colspan="2" height="10"></td>
                                </tr>
                                <tr valign="top">
                                    <td align="right"><b>Email Text:&nbsp;&nbsp;</b></td>
                                    <td><textarea name="email_body" rows="15" cols="80">[% email_content %]</textarea></td>
                                </tr>
                                <tr>
                                    <td colspan="2" height="10"></td>
                                </tr>
                                <tr>
                                    <td colspan="2" class="divider"></td>
                                </tr>
                            </table>
                            <br><br>
                        [% END %]
                    [% END %]
                [% END %]

            [% IF subsection == "Invalid Shipments" %]

                [% IF routing_request.size %]
                <span class="title title-[% channel_config.$sales_channel %]">DHL Routing Request Log</span><br>

                      <table width="100%" cellpadding="0" cellspacing="0" border="0" class="data">
                        <tr>
                          <td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                        </tr>
                        <tr>
                          <td class="tableHeader">&nbsp;&nbsp;Date</td>
                          <td class="tableHeader">Error Code</td>
                          <td class="tableHeader">Message</td>
                        </tr>
                        <tr>
                          <td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                        </tr>

                        [% FOREACH sort = routing_request.keys.nsort.reverse %]
                            <tr>
                              <td>&nbsp;&nbsp;[% routing_request.$sort.date | html %]</td>
                              <td>[% routing_request.$sort.error_code | html %]</td>
                              <td>[% routing_request.$sort.error_message | html %]</td>
                            </tr>
                            <tr>
                              <td colspan="3" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                            </tr>
                        [% END %]

                      </table>

                 [% END %]
            [% END %]

            <br /><br />
            <table width="100%" cellpadding="0" cellspacing="0" border="0">
                <tr>
                    <td align="right">
                    [% IF from_select_shipping_option && address_type == "Shipping" %]
                        [% INCLUDE page_elements/forms/submit_button.tt submit = 'Select Shipping Option &raquo;' %]
                    [% ELSE %]
                        [% INCLUDE page_elements/forms/submit_button.tt submit = 'Confirm Changes &raquo;' %]
                    [% END %]
                    </td>
                </tr>
            </table>
            </form>

        [% ELSE # !new_address.size %]

        [% IF restrictions %]

                <span class="title title-[% channel_config.$sales_channel %]">Shipment Items</span><br>

                <table width="100%" cellpadding="0" cellspacing="0" border="0" class="data">

                    <tr>
                        <td colspan="6" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
                    </tr>
                    <tr>
                        <td class="tableHeader" width="3%"></td>
                        <td class="tableHeader" width="10%">PID</td>
                        <td class="tableHeader" width="16%">Designer</td>
                        <td class="tableHeader" width="19%">Name</td>
                        <td class="tableHeader" width="10%">Size</td>
                        <td class="tableHeader" width="19%">Restriction</td>
                    </tr>
                    <tr>
                        <td colspan="6" class="dividerHeader"><img src="/images/blank.gif" width="1" height="1"></td>
                    </tr>

                    [% FOREACH id = shipment_items.keys.nsort %]

                        [% IF shipment_items.$id.shipment_item_status_id == SHIPMENT_ITEM_STATUS__NEW
                           || shipment_items.$id.shipment_item_status_id == SHIPMENT_ITEM_STATUS__SELECTED
                           || shipment_items.$id.shipment_item_status_id == SHIPMENT_ITEM_STATUS__PICKED
                           || shipment_items.$id.shipment_item_status_id == SHIPMENT_ITEM_STATUS__PACKING_EXCEPTION
                           || shipment_items.$id.shipment_item_status_id == SHIPMENT_ITEM_STATUS__PACKED %]

                            [%-
                                SET product_restricted  = ( shipment_items.$id.restricted ? 1 : 0 );
                                SET product_notify      = ( shipment_items.$id.restricted_details.actions.notify ? 1 : 0 );
                                SET product_reasons     = shipment_items.$id.restricted_details.reason_descriptions.join( ', ' );
                                SET row_title           = ( product_restricted ? "Can't Deliver" : "Can Deliver" );
                            -%]
                            <tr class="[% IF product_restricted %]restricted_highlight[% ELSE %]not_restricted_highlight[% END %]" title="[% row_title %]">
                                <td>&nbsp;<img class="inline" src="/images/icons/[% IF product_restricted %]lorry_error[% ELSE %]lorry[% END %].png"></td>
                                <td>[% shipment_items.$id.sku %]</td>
                                <td>[% shipment_items.$id.designer %]</td>
                                <td>[% shipment_items.$id.name %]</td>
                                <td>[% shipment_items.$id.size %]</td>
                                <td>
                                    [%- IF product_restricted -%]
                                        [%- product_reasons -%]
                                    [%- ELSE -%]
                                        No Restrictions
                                        [%- IF product_notify -%]
                                            &nbsp;-&nbsp;[% product_reasons %] (requires a notification)
                                        [%- END -%]
                                    [%- END -%]
                                </td>
                            </tr>
                            <tr>
                                <td colspan="6" class="divider"><img src="/images/blank.gif" width="1" height="1"></td>
                            </tr>

                        [% END # if %]

                    [% END # shipment_item%]

                </table>

            [% END # restrictions %]

        [% END %]
