<h3 class="title-[% channel_config.$sales_channel %]">Shipment Items</h3>
<table class="data wide-data divided-data">
    <thead>
        <tr>
            <th>&nbsp;</th>
            <th>SKU</th>
            <th>Name</th>
            <th>Designer</th>
            <th>Size</th>
            <th>Status</th>
            <th>Unit price</th>
            <th>Tax</th>
            <th>Duties</th>
            <th>Sub-Total</th>
        </tr>
    </thead>
    <tbody>
    [% FOREACH itemid = ship_items.keys %]
        [% class = "" %]

        [% IF ship_items.$itemid.shipment_item_status_id == db_constant('SHIPMENT_ITEM_STATUS__CANCEL_PENDING')
           || ship_items.$itemid.shipment_item_status_id == db_constant('SHIPMENT_ITEM_STATUS__CANCELLED')
           || ship_items.$itemid.shipment_item_status_id == db_constant('SHIPMENT_ITEM_STATUS__LOST')
           || ship_items.$itemid.shipment_item_status_id == db_constant('SHIPMENT_ITEM_STATUS__UNDELIVERED');
            class = " class=\"light\"";
        END %]

        <tr>
            <td[% class %] align="center">
                <a href="[% ship_items.$itemid.image.0 %]" target="new" class="imagezoom" title="View Product Image">
                    <img src="/images/icons/image.png" />
                </a>
            </td>
            <td[% class %]>[% ship_items.$itemid.product_id _ '-' _ ship_items.$itemid.size_id %]</td>
            <td[% class %]>[% ship_items.$itemid.name %]</td>
            <td[% class %]>[% ship_items.$itemid.designer %]</td>
            <td[% class %]>[% ship_items.$itemid.size %]</td>
            <td[% class %]><span class="highlight">[% ship_items.$itemid.status %]</td>
            <td[% class %] align="right">[% ship_items.$itemid.unit_price %]&nbsp;&nbsp;&nbsp;</td>
            <td[% class %] align="right">[% ship_items.$itemid.tax %]&nbsp;</td>
            <td[% class %] align="right">[% ship_items.$itemid.duty %]&nbsp;</td>
            <td[% class %] align="right">[% ship_items.$itemid.sub_total %]&nbsp;</td>
        </tr>
    [% END %]
