[% IF ship_info.shipment_status_id == db_constant('SHIPMENT_STATUS__FINANCE_HOLD') %]
    <span class="error_msg">This shipment has been placed on Financial Hold.</span>
[% ELSIF ship_info.shipment_status_id == db_constant('SHIPMENT_STATUS__HOLD') %]
    <span class="error_msg">This shipment has been placed on Hold.</span>
[% ELSIF ship_info.shipment_status_id == db_constant('SHIPMENT_STATUS__DISPATCHED') %]
    <span class="error_msg">This shipment has already been dispatched.</span>
[% ELSIF ship_info.shipment_status_id == db_constant('SHIPMENT_STATUS__CANCELLED') %]
    <span class="error_msg">This shipment has been cancelled.</span>
[% ELSE %]

    [% PROCESS page_elements/printer_station_name.tt %]
    <span class="title title-[% channel_config.$sales_channel %]">
        [%- IF subsection == "Courier Reference" -%]
            Enter Courier Reference
        [%- ELSE -%]
            Enter Airwaybill
        [%- END -%]
    </span><br />

    <form name="allocateAirwayForm" action="/Fulfilment/Airwaybill/UpdateShipmentAirwaybill" method="post">
    <input type="hidden" name="shipment_id" value="[% shipment_id %]">
    <table width="100%" cellpadding="0" cellspacing="0" border="0" class="data">
        <tr>
            <td colspan="3" class="divider"></td>
        </tr>
        <tr height="24">
            <td width="18%" align="right">
                <b>[% IF subsection == "Courier Reference" %]Reference Number[% ELSE %]Outward Airwaybill[% END %]:</b>&nbsp;&nbsp;&nbsp;
            </td>
            <td width="82%">
                <input type="text" name="out_airway" value="[% ship_info.outward_airway_bill IF ship_info.outward_airway_bill != "none" %]" maxlength="[% waybill_length %]">
            </td>
        </tr>
        <tr>
            <td colspan="2" class="divider"></td>
        </tr>
        [% IF subsection != "Courier Reference" && is_returnable %]
        <tr height="24">
            <td width="18%" align="right"><b>Return Airwaybill:</b>&nbsp;&nbsp;&nbsp;</td>
            <td width="82%">
                <input type="text" name="ret_airway" value="[% ship_info.return_airway_bill IF ship_info.return_airway_bill != "none" %]" maxlength="[% waybill_length %]">
            </td>
        </tr>
        <tr>
            <td colspan="2" class="divider"></td>
        </tr>
        [% END %]
        <tr>
            <td colspan="2" class="blank"><img src="/images/blank.gif" width="1" height="10"></td>
        </tr>
        <tr>
            <td colspan="2" class="blank" align="right">[% INCLUDE page_elements/forms/submit_button.tt %]</td>
        </tr>
    </table>
    </form>

    <script language="Javascript">
    <!--
        [% IF ship_info.outward_airway_bill == "none" %]

            document.allocateAirwayForm.out_airway.focus();
        [% ELSIF ship_info.return_airway_bill == "none" && ship_info.outward_airway_bill != "none" && is_returnable %]

            document.allocateAirwayForm.ret_airway.focus();
        [% END %]
    //-->
    </script>

    <script language="Javascript">

        function validate(f){

            var airway = /\b\w?\d{10,11}\b/i;
            var airway_alt = /\b\d{1}\w{1}\d{1}\w{2}\d{13}\b/i;

            var error = 0;

            if(f.out_airway.value != ""){
                if (airway.test(f.out_airway.value) || airway_alt.test(f.out_airway.value)){
                    error = 0;
                }
                else {
                    error = 1;
                }
            }

            if(f.ret_airway.value != ""){
                if (airway.test(f.ret_airway.value) || airway_alt.test(f.ret_airway.value)){
                    error = 0;
                }
                else {
                    error = 1;
                }
            }


            if( error == 0 ) {
                return true;
            }
            else{
                alert("The Airwaybill entered was not in the correct format, please try again.");
                return false;
            }
        }


    </script>

    <br /><br /><br />

[% END %]

