                                                [% IF list_type == "Live" %]<b>[% IF  is_reservation_editing_allowed %]<a href="javascript:void(0);" style="text-decoration: none" id="single_[% cust_id %]_[% reserv_id %]" class="singleplusminus_button" name="minus"> - </b></a>
                                                    <input [% IF current_date > list.$channel.$op_id.$cust_id.$reserv_id.check_date || 0 %]style="border: 2px solid #CC0000"[% END %] id="single_[% cust_id %]_[% reserv_id %]" class="single_datepicker dateadj_[% cust_id %]" type="text" name="expiry-[% list.$channel.$op_id.$cust_id.$reserv_id.id %]" value="[% list.$channel.$op_id.$cust_id.$reserv_id.expiry_ddmmyy %]" autocomplete="no" size="8" onchange="ValidateDate(this)"/>
                                                    <input type="hidden" name="original_expiry_[% reserv_id %]" value="[% list.$channel.$op_id.$cust_id.$reserv_id.expiry_ddmmyy %]">
                                               <b> <a href="javascript:void(0);" style="text-decoration: none" id="single_[% cust_id %]_[% reserv_id %]" class="singleplusminus_button" name="plus"> + </b></a> [% ELSE %] [% list.$channel.$op_id.$cust_id.$reserv_id.expiry_ddmmyy %] [% END %]
                                                [% ELSE %]
                                                    [% list.$channel.$op_id.$cust_id.$reserv_id.creation_date %]
                                                [% END %]
                                            </td>
                                            [% IF list_type == "Live" && filter != 'All' %]
                                            <td>
                                               [% IF  is_reservation_editing_allowed %] <input type="checkbox" class ="bulk_checkboxes_[% channel_config.$channel %] subselect_bulk_checkbox_[% cust_id %]_[% op_id %]" id="s_[% cust_id %]_[% reserv_id %]" name="update-[% list.$channel.$op_id.$cust_id.$reserv_id.id %]" value="1" /> [% END %]
                                            </td>
                                            [% END %]
                                            <td>
                                               [% IF is_reservation_editing_allowed %]<input type="checkbox" class ="subselect_checkbox_[% cust_id %]_[% op_id %]" name="delete-[% list.$channel.$op_id.$cust_id.$reserv_id.id %]" value="1" /> [% ELSE %] <img src="/images/ps_dept.png" height="20" title="Personal shopping department"> [% END %]
                                            </td>

                                        </tr>
                                        <tr>
                                            <td colspan="9" class="divider"></td>
                                        </tr>
                                    [% END %]
                                </table>
                                <br>

                            </div>

                        [% END %]

