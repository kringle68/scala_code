[%# Wrapper file for all XTracker templates -%]
[%-

    IF template_type == 'none' || template_type == 'csv' || template_type == 'email' || template_type == 'email_subject';
        # no wrappers. Just the content;
        content;

    ELSIF template_type == 'blank';
        content WRAPPER shared/layout/blank;

    ELSIF template_type == 'handheld';
        content WRAPPER shared/layout/handheld;

    ELSE;
        # default main wrappers
        content WRAPPER shared/layout/page WRAPPER shared/layout/html;

    END;
-%]
