<html> 
<head> 
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<!-- Template name: [% template.name %] -->
<title></title>
<style type="text/css">
<!-- 
body { margin-top: 1cm; margin-left: 0.5cm; }
//-->
</style>
</head>
<body bgcolor="#FFFFFF" TEXT="#000000">

<table border=0 cellspacing=0 cellpadding=0 width=570>
	<tr>
		<td colspan=2 align="center" height="75" valign="top">
			<font face="Arial,Helvetica" size=4 color="#000000">N E T - A - P O R T E R . C O M </font><br>
			<!-- img src="/images/blank.gif" width=570 height=50 border=0 -->
		</td>
	</tr>
	<tr valign="top">
		<td>
			<font face="Arial,Helvetica" size=2 color="#000000"><b>DELIVERY NOTE</b><br>
		</td>
		<td align="right">
			<font face="Arial,Helvetica" size=2 color="#000000">[% date %]<br>
		</td>
	</tr>
	<tr valign="top">
		<td colspan=2 height="50">
			<!-- img src="/images/blank.gif" width=600 height=50 border=0 -->
		</td>
	</tr>
	<tr>
		<td bgcolor="#999999"><img src="/images/blank.gif" width=150 height=1 border=0></td>
		<td bgcolor="#999999"><img src="/images/blank.gif" width=420 height=1 border=0></td>
	</tr>
	<tr height="22">
		<td><font face="Arial,Helvetica" size=-1 color="#000000">&nbsp;<b>Order Number:&nbsp;</b></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% order.order_nr %]</font></td>
	</tr>
	<tr>
		<td colspan=5 bgcolor="#999999"><img src="/images/blank.gif" width=1 height=1 border=0></td>
	</tr>
	<tr height="22">
		<td><font face="Arial,Helvetica" size=-1 color="#000000">&nbsp;<b>Order Date:&nbsp;</b></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% shipment.date %]</font></td>
	</tr>
	<tr>
		<td colspan=5 bgcolor="#999999"><img src="/images/blank.gif" width=1 height=1 border=0></td>
	</tr>
	<tr>
		<td colspan=5><img src="/images/blank.gif" width=1 height=5 border=0></td>
	</tr>
	<tr>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">&nbsp;<b>Address:</b></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% shipping_address.first_name %]&nbsp;[% shipping_address.last_name %]</font></td>
	</tr>
	<tr>
		<td></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% shipping_address.address_line_1 %][% IF shipping_address.address_line_2 != "" %], [% shipping_address.address_line_2 %][% END %][% IF shipping_address.address_line_3 != "" %], [% shipping_address.address_line_3 %][% END %]</font></td>
	</tr>
	<tr>
		<td></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% shipping_address.towncity %]</font></td>
	</tr>
	<tr>
		<td></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% shipping_address.county %]</font></td>
	</tr>
	<tr>
		<td></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% shipping_address.postcode %]</font></td>
	</tr>
	<tr>
		<td></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% shipping_address.country %]</font></td>
	</tr>
	<tr>
		<td colspan=5><img src="/images/blank.gif" width=1 height=10 border=0></td>
	</tr>
	<tr>
		<td colspan=5 bgcolor="#999999"><img src="/images/blank.gif" width=1 height=1 border=0></td>
	</tr>
	<tr>
		<td height="22"><font face="Arial,Helvetica" size=-1 color="#000000">&nbsp;<b>Telephone:</b></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% shipment.telephone %]</font></td>
	</tr>
	<tr>
		<td colspan=5 bgcolor="#999999"><img src="/images/blank.gif" width=1 height=1 border=0></td>
	</tr>
	<tr>
		<td height="22"><font face="Arial,Helvetica" size=-1 color="#000000">&nbsp;<b>Mobile Telephone:</b></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% shipment.mobile_telephone %]</font></td>
	</tr>
	<tr>
		<td colspan=5 bgcolor="#999999"><img src="/images/blank.gif" width=1 height=1 border=0></td>
	</tr>
	<tr>
		<td colspan=5><img src="/images/blank.gif" width=1 height=30 border=0></td>
	</tr>

	[% IF note %]
	<tr>
		<td colspan=5 bgcolor="#999999"><img src="/images/blank.gif" width=1 height=1 border=0></td>
	</tr>
	<tr>
		<td height="22"><font face="Arial,Helvetica" size=-1 color="#000000">&nbsp;<b>Delivery Note:</b></td>
		<td><font face="Arial,Helvetica" size=-1 color="#000000">[% note %]</font></td>
	</tr>
	<tr>
		<td colspan=5 bgcolor="#999999"><img src="/images/blank.gif" width=1 height=1 border=0></td>
	</tr>
	<tr>
		<td colspan=5><img src="/images/blank.gif" width=1 height=30 border=0></td>
	</tr>
	[% END %]

	<tr valign="top">
		<td colspan=5>
			<font face="Arial,Helvetica" size=2 color="#000000"><b>Items</b><br>
			<br>
			</font>
		</td>
	</tr>
</table>

<table border=1 cellspacing=0 cellpadding=0 width=600 bordercolor="#ffffff">
	<tr bgcolor="#cccccc">
		<td><font face="Arial,Helvetica" size=1 color="#000000">&nbsp;DESCRIPTION</font></td>
		<td align="center"><font face="Arial,Helvetica" size=1 color="#000000">QTY</font></td>				
	</tr>
	[% FOREACH id = shipment_item.keys %]
	<tr bgcolor="#eeeeee" height="40">
		<td width="80%">&nbsp;<font face="Arial,Helvetica" size=1 color="#000000">[% shipment_item.$id.designer %]&nbsp;&nbsp;[% shipment_item.$id.name %]</font></td>
		<td align="center"><font face="Arial,Helvetica" size=1 color="#000000">[% shipment_item.$id.quantity %]</font></td>				
	</tr>
	[% END %]
</table>

<br><br><br><br><br>
<br><br>
<table border=0 cellspacing=0 cellpadding=0>
	<tr>
		<td><font face="Arial,Helvetica" size=1 color="#000000"><b>&nbsp;NAME</font></td>
		<td><img src="/images/blank.gif" width=50 height=30 border=0></td>
		<td><font face="Arial,Helvetica" size=1 color="#000000"><b>&nbsp;SIGNATURE</font></td>
		<td><img src="/images/blank.gif" width=50 height=30 border=0></td>
		<td><font face="Arial,Helvetica" size=1 color="#000000"><b>&nbsp;DATE</font></td>			
	</tr>
	<tr>
		<td>
			<table border=1 cellspacing=0 cellpadding=0 width=200 bordercolor="#000000">
				<tr>
					<td><img src="/images/blank.gif" width=200 height=50 border=0></td>			
				</tr>
			</table>
		</td>
		<td><img src="/images/blank.gif" width=1 height=30 border=0></td>
		<td>
			<table border=1 cellspacing=0 cellpadding=0 width=200 bordercolor="#000000">
				<tr>
					<td><img src="/images/blank.gif" width=200 height=50 border=0></td>			
				</tr>
			</table>
		</td>
		<td><img src="/images/blank.gif" width=1 height=30 border=0></td>
		<td>
			<table border=1 cellspacing=0 cellpadding=0 width=120 bordercolor="#000000">
				<tr>
					<td><img src="/images/blank.gif" width=120 height=50 border=0></td>			
				</tr>
			</table>
		</td>		
	</tr>
</table>


</body>
</html>
