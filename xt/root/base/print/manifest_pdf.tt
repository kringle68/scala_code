<!-- Template name: [% template.name %] -->
<body>
<div align='center'>

[% SET header_loop = 15	# set the number of records per page before including header row in PDF %]  		
[% SET loop_count = 0		# set up a loop counter %] 				
[% SET page_record = 0	# flag to keep track of how many records are on current page	 %]


[% FOREACH shipment_id = shipment_data.keys.nsort %]

	[%# add page header if first record on a page %]
	[% IF loop_count % header_loop == 0 %]
		
		[% IF loop_count != 0 %]
			</table>
                        <div style="page-break-after: always;"><span style="display: none;">&nbsp;</span></div>
                        <br><br><br>
		[% END %]

	        [% IF dc_name == 'DC3' %]	
                        DHL Express (Hong Kong) Limited<br>
                        Level 26, Tower 1, Enterprise Square Five<br>
                        38 Wang Chiu Road, Kowloon Bay, Hong Kong<br>
	        [% ELSE %]	
		        DHL International GmbH<br>
		        DHL EXPRESS (UK) LTD, ORBITAL PK, 178-188 GT STH<br>
		        WEST RD, HOUNSLOW, MIDDX, TW4 6JS, CO. No: 2275780<br>
	        [% END %]	
		<br>
			
		<table cellpadding='3' cellspacing='0' border='1'>
			<tr>
				<td><font size='1'><b>Bill to<br />Account</b></td>
				<td><font size='1'><b>Licence<br />Plate No.</b></td>
				<td><font size='1'><b>Airbill<br />Number</b></td>
				<td><font size='1'><b>Shipper<br />Reference</b></td>
				<td><font size='1'><b>Consignee<br />Airport</b></td>
				<td><font size='1'><b>Airbill<br />Pieces</b></td>
				<td><font size='1'><b>Airbill<br />Weight</b></td>
				<td><font size='1'><b>Airbill<br />Dim Weight</b></td>
				<td align="left"><font size='1'><b>Receiver Company/Name</b></td>
				<td><font size='1'><b>Airbill<br />Contents</b></td>
				<td><font size='1'><b>Prod<br />Type</b></td>
				<td><font size='1'><b>Customs<br />Value</b></td>
			</tr>


		[% page_record = 0 %]
	[% END %]
	
	[%# shipment table row %]
	<tr>
		<td width="5%"><font size='1'>[% shipment_data.$shipment_id.account_number %]<br><br></font></td>
		<td width="15%"><font size='1'>[% FOREACH box_id = shipment_data.$shipment_id.boxes.keys %]JD00[% shipment_data.$shipment_id.boxes.$box_id.tracking_number %]<br />[% END %]</font></td>
		<td width="5%"><font size='1'>[% shipment_data.$shipment_id.outward_airway_bill %]</font></td>
		<td width="5%"><font size='1'>[% shipment_data.$shipment_id.order_nr %]</font></td>
		<td width="5%"><font size='1'>[% shipment_data.$shipment_id.destination_code %]</font></td>
		<td width="5%"><font size='1'>[% shipment_data.$shipment_id.num_boxes %]</font></td>
		<td width="5%"><font size='1'>[% shipment_data.$shipment_id.total_weight %]</font></td>
		<td width="10%"><font size='1'>[% shipment_data.$shipment_id.total_volumetric_weight %]</font></td>
		<td width="15%" align="left"><font size='1'>[% shipment_data.$shipment_id.customer_name FILTER upper %]</font></td>
		<td width="20%"><font size='1'>[% shipment_data.$shipment_id.description_of_goods %]</font></td>
		<td width="5%"><font size='1'>[% shipment_data.$shipment_id.product_code %]</font></td>
		<td width="5%"><font size='1'>[% shipment_data.$shipment_id.total_value FILTER format('%02.2f') %] [% shipment_data.$shipment_id.currency %]</font></td>
	</tr>

	[% loop_count = loop_count + 1 %]
	[% page_record = page_record + 1 %]
[% END %]

</table>
</div>

<div style="page-break-after: always;"><span style="display: none;">&nbsp;</span></div>
