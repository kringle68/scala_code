<html>
    <head>
    <!-- Template name: [% template.name %] -->
    <title></title>
    <script type="text/javascript" src="/jquery/jquery-1.7.min.js"></script>
    <script type="text/javascript">
        $(document).ready( function() {
            // fix static image paths
            $("img[src*='/xt/root/static/']").each(function(i){$(this).attr('src',$(this).attr('src').replace(/^.*\/xt\/root\/static/,''))});
            // fix static print document paths
            $("img[src*='/xt_static/']").each( function(i){$(this).attr('src', $(this).attr('src').replace(/^.*\/xt_static/,''));});
        });
    </script>
    </head>
<body>
    <table align="center"><tr><td>
    [% IF config_section == 'NAP' %]<br/><br/><br/><br/>[% END %]  <!-- push nap text down because the logo is at the top -->
    <img src="[% full_img_path %]" height="250px">
    </td>
    [% IF config_section == 'NAP' %]<td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>[% END %] <!-- push it left slightly too -->
    </tr></table>
</body>
</html>

