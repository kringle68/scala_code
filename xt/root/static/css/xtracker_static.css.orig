/* xtracker_static.css contains styles for XTracker webapp page layout and design.
 * Other css files exist, but the intention is that this will be the central
 * place for all xtracker-specific styling
 *
 * All work copyright net-a-porter.com.
 *
 * The following describes the intended layout of this CSS file. Depending
 * on time spent refactoring, it probably won't actually resemble the file
 * layout, but it's what we should be moving towards
 *
 *
 * The file is organised into broad sections as follows:
 *  * General styles
 *    - Reset declarations
 *    - base element styles
 *
 *  * Helper styles
 *    - Generic helper classes eg notifications and errors
 *    - Normally just one class which affecting one element.
 *
 *  * Page structure
 *    - Skeleton inc page furniture
 *    - Header, footer, navigation elements and related styling
 *
 *  * Page components
 *    - Will be reusable components and widgets
 *    - Probably most styles will be here
 *
 *  * Page/Section specific styling
 *    - Such as widths for tables in certain sections of the site
 *
 *  * Overrides
 *    - Override general cases with specific non-standard behaviour here
 */

/*********************************************
 * General Styles
 *********************************************/
html,body,div,form,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,th,td {
    margin:               0;
    padding:              0;
    background:           transparent;
}

body {
    font : 100.01% Arial, "Nimbus Sans L", Avenir, "Lucida Grande", Verdana, "Bitstream Vera Sans", Helvetica, sans-serif;
	background: #fff;
	color: #333;
    text-align: left;
    margin-bottom: 10px;
    min-width: 960px;
}

img	{
	display: block;
	border: 0px;
}
/* This is for the datepicker jq plugin */
img.ui-datepicker-trigger {
    display: inline;
}
img.center {
  margin-left:  auto;
  margin-right: auto;
}

h1, h2, h3, h4, h5 {
	margin: 0 10px 0 0;
	padding: 0;
	color: #777;
	font-weight: bold;
        clear: both;
}

h1 {
  	font-size: 1.4em;
}

h2 {
  	font-size: 1.2em;
}

h3 {
  	font-size: 1.1em;
    line-height: 2.2em
}

h4 {
	color: #444;
  	font-size: 1.1em;
}

h5 {
	color: #bbb;
  	font-size: 1.1em;
}

a {
    text-decoration: underline;
    color: #444;
}
a:hover {
    text-decoration: none;
	color: #000;
}

/* default form styling */
input, select, textarea {
    font-family : inherit;
}
input[type="checkbox"], input[type="radio"] {
    vertical-align: middle;
}


li {
    list-style-position:  inside;
    margin-left: 10px;
}


/*********************************************
 * Helper styles
 *********************************************/

img.product {
	display: inline;
	border: 1px solid #999999;
	float: left;
	margin-right: 4px;
}

img.inline {
    display: inline;
    vertical-align: bottom;
    margin: 0 5px;
}

a.highlight {
    text-decoration: underline;
    color: #000;
}
a.pagenum {
    text-decoration: none;
	color: #666;
}
a.pagenum:hover {
    text-decoration: none;
	color: #000;
}

.postclear:after {
    content:        ".";
    display:        block;
    clear:          both;
    visibility:     hidden;
    height:         0;
    line-height:    0;
}

.title {
    line-height: 2.2em;
    padding: 0;
    color: #777;
    font-weight: bold;
    font-size: 1.1em;
}

.showicon {
    padding-left: 15px;
    background: transparent url(/images/icons/extlink.gif) no-repeat center left;
}

.info, .message, .display_msg, .warning, .error_msg, .debug {
    display:            block;
    line-height:        1.7em;
    padding:            5px 5px 5px 30px;
    margin-bottom:      0 0 10px 0;
}
.info, .message {
    background: url(/images/icons/information.png) #EEE 10px 7px no-repeat;
    color:      #000;
    font-weight:normal;
    border:     1px dashed #999;
}
.debug {
    background: url(/images/icons/information.png) #EA6 10px 7px no-repeat;
    color:      #000;
    font-weight:normal;
    border:     1px dashed #999;
    font-family:monospace, fixed;
    white-space:pre;
    overflow:   scroll;
    height:     20em;
    font-size:  0.9em;
    line-height:1.2em;
}
.message {
    background-image : url(/images/icons/email.png);
}
.display_msg {
    background: url(/images/icons/accept.png) #CFC 10px 7px no-repeat;
    color:      #006600;
    font-weight:bold;
    border:     1px dashed #060;
}
.warning {
    background: url(/images/icons/cancel.png) #930 10px 7px no-repeat;
    color:      #FFF;
    font-weight:bold;
}
.error_msg {
    background: url(/images/icons/cancel.png) #FCC 10px 7px no-repeat;
    color:      #C00;
    font-weight:bold;
    border:     1px dashed #C00;
    white-space: -moz-pre-wrap !important;  /* Mozilla, since 1999 */
    white-space: -pre-wrap;                 /* Opera 4-6 */
    white-space: -o-pre-wrap;               /* Opera 7 */
    white-space: pre-wrap;                  /* css-3 */
    word-wrap: break-word;                  /* Internet Explorer 5.5+ */
}
.error_msg span {
    width: 100%;
}

.good {
    color : #090;
}

.nowrap {
    white-space: nowrap;
}

.spaced {
    margin: 10px 0;
}
.spaced_under {
    margin-bottom: 20px;
}


/* smaller form elements */
#content input.smaller {
  width:            100px;
  min-width:        100px;
}
#content input.small {
  width:            60px;
  min-width:        60px;
}
#content input.smallest {
  min-width:        0px;
  width:            auto;
}
#content input.larger {
  min-width:        212px;
  width:            212px;
}

/* Highlight success or error actions on inputs */
input.error, input.bad {
    background : #F99;
    margin: 5px;
}
input.success, input.good {
    background : #9F9;
    margin: 5px;
}

input.large_button {
  min-width:        212px;
  width:            212px;
  padding:          1em;
}


.divideabove, .formrow.formstart, tr.divideabove td, tr.divideabove th, table.wide-data th, table.divided-data td {
  border-top:       1px dotted #999;
}
.dividebelow, .formrow.formend, tr.dividebelow td, tr.dividebelow th, table.wide-data th, table.divided-data td {
  border-bottom:    1px dotted #999;
}
.divideright {
  border-right:     1px dotted #999;
}
.divideleft {
  border-left:      1px dotted #999;
}

table.divided-data tr.separator td{
    border-bottom: 1px solid #999999;
}

.nodividebelow, tr.nodividebelow td, tr.nodividebelow th {
  border-bottom:    0px;
}

/* Adds space above element, equal to 1 or 2 lines */
.with-1-line-above {
    margin: 1em 0 0 0;
}

.with-2-line-above {
    margin: 2em 0 0 0;
}

/*********************************************
 * Page structure
 *********************************************/

#container	{
	width: 960px;
	height: 100%;
	margin-left: auto;
	margin-right: auto;
}

#header	{
	padding-bottom:1px;
	border-bottom: 1px dotted #666;
}

#headerTop	{
    position: relative;
	height: 41px;
    z-index: 2;
}

#headerTop #headerLogo {
	float: left;
    font-weight: bold;
    font-size: 10px;
    margin:0;
}
#headerTop #headerLogo span {
    color: #999;
    border-right: 1px solid #CCC;
    padding: 0 7px 0 40px;
}
#headerTop #headerLogo span.dc {
	color: #333;
    border: 0;
    padding: 0 0 0 7px;
}

#headerTop #headerControls {
    position: absolute;
    right: 17em;
    top: 12px;
    font-size: 11px;
}
#headerTop #headerControls span {
    color: #777;
}
#headerTop #headerControls img {
    display: inline;
    vertical-align: bottom;
}
#headerTop #headerControls .messages {
    padding: 0 10px;
    margin: 0 15px;
    border: solid #CCC;
    border-width: 0 1px;
}
#headerTop #headerControls a {
    text-decoration: none;
    color: #000;
}
#headerTop select {
    position: absolute;
    right: 0;
    top: 10px;
    font-size: 11px;
}

#headerBottom	{
    position: relative;
	height: 65px;
	background-image: url(/images/header_bg.gif);
	margin-bottom: 1px;
}
#headerBottom img {
    position: absolute;
    right: 5em;
    bottom: 0;
}

#nav1 {
	background: #494949;
    font-size: 0.7em;
    min-height: 21px;
}
#nav1 li {
    list-style-image:     none;
    list-style-position:  outside;
    list-style-type:      none;
}
#nav1 li, #nav1 li a, #nav1 span {
    border-color: #B0B1AC;
    color: #B0B1AC;
}
#nav1 span {
    display: block;
    text-align: center;
    font-weight: bold;
    color : #FFFF55;
    padding-left: 5px;
}
#nav1 li a {
    border-top:0;
    padding: 0 5px;
}
#nav1 li a.yuimenubaritemlabel-hassubmenu {
    padding-right: 16px;
}
#nav1 .yuimenuitem-selected,
#nav1 .yuimenubaritem-selected {
    background: #666;
}
#nav1 li ul {
    padding: 5px 0;
}
#nav1 li ul li a {
    color: #777;
    padding: 0 15px 0 10px;
}
#nav1 li ul li.yuimenuitem-selected,
#nav1 li ul li.yuimenubaritem-selected {
    background: #EEE;
}
#nav1 li ul li.yuimenuitem-selected a,
#nav1 li ul li.yuimenubaritem-selected a {
    color: #000;
}
#nav1 .yuimenubaritemlabel-hassubmenu {
    background-image : url(/images/menubaritem_submenuindicator.png);
}

#content {
    position: relative;
  	font-size: 0.7em;
    margin: 20px 0;
}

#contentLeftCol {
	float: left;
	padding: 0 1px 0 0;
	width: 119px;
	border-right: 1px dotted #bbb;
    /* 1px top margin makes dots of border align with
       dots on lhs of contentRight!  */
    margin: 1px -1px 0 0;
	text-align: right;
}
#contentLeftCol ul li {
    list-style-image:     none;
    list-style-position:  outside;
    list-style-type:      none;
    margin: 0;
}
#contentLeftCol ul li * {
	font-weight: bold;
    border-bottom: 1px dotted #bbb;
    padding: 5px 5px 2px 0;
    clear: right;
    float: right;
}
#contentLeftCol ul li .last {
    margin-bottom: 10px;
}
#contentLeftCol ul li a {
    text-decoration: none;
    font-weight: normal;
	color: #777;
}
#contentLeftCol ul li a:hover {
	color: #000;
}

#channelTitle {
    position: absolute;
    top: -15px;
    right: 0;
}

#contentRight {
    float: left;
    width: 820px;
	border-left: 1px dotted #bbb;
	padding-left: 19px;
    margin-bottom: 25px;
    min-height: 300px;
}

#contentRight.noleftcol {
    float: none;
    margin-left: 120px;
}

#pageTitle {
	margin: 0 250px 5px 0;
}

#pageTitle h1,
#pageTitle h2,
#pageTitle h3,
#pageTitle h4,
#pageTitle h5 {
    display: inline;
}

#footer	{
    clear: both;
	background-color: #494949;
	color: #fff;
    font-size: 0.7em;
	margin: 0;
	padding: 5px;
	text-align: right;
}


/*********************************************
 * Page components
 *********************************************/

/********* Form Components */
/* formrow group is a widget to align form elements */
.formrow {
    clear:            both;
    padding:          3px 0 3px 160px;
    background-color: #F2F2F2;
}
.formrow:after {
    content:        ".";
    display:        block;
    clear:          both;
    visibility:     hidden;
    height:         0;
    line-height:    0;
}
.formrow.formstart {
    margin-top:     7px;
}
.formrow.formend {
    margin-bottom:  25px;
}
.formrow.buttons {
    padding-top:    13px;
    margin-bottom:  10px;
}
.formrow.aftertable,
.formrow.beforetable {
    background-color: #FFF;
    text-align:       right;
}
.formrow.aftertable {
    margin-top:     0;
}
.formrow.beforetable {
    margin-bottom:  0;
}
.formrow.aftertable label {
    float:          none;
    display:        inline;
}


.formrow label, .formrow .fakelabel {
    float:          left;
    text-align:     right;
    width:          150px;
    margin-left:    -160px;
    padding:        3px 0;
    color:          #333;
    font-weight:    bold;
}
.formrow p {
    padding:        3px 0;
    margin:         0;
}
.formrow input {
    width: 155px;
}
.formrow select, .formrow input[type="button"], .formrow input[type="submit"] {
    width: auto;
    min-width: 161px;
}

.formrow input[type="radio"] {
  width:            auto;
}

.formrow label.for-radio, .formrow label.for-checkbox {
  color:            #000;
  font-size:        100%;
  font-weight:      400;
  float:            none;
  margin-left:      0 !important;
  margin-right:     10px;
  width:            auto;
  line-height:      100%;
}

.formrow.narrowlabel, .narrowlabels .formrow {
    padding-left: 80px;
}
.formrow.narrowlabel input, .narrowlabels .formrow input{
    width: 120px;
}
.formrow.narrowlabel input[type="button"],
.formrow.narrowlabel input[type="submit"],
.narrowlabels .formrow input[type="button"],
.narrowlabels .formrow input[type="submit"] {
    min-width: 126px;
    width: 126px;
}


table.wide-data {
  width: 100%;
  border-collapse: collapse;
}
table.wide-data td, table.wide-data th {
  padding: 0.5em 0 0.5em 1em;
}
td div.bottom-half {
  padding: 0.5em 0 0 0;
}

/* # tableHeader is the old class and should NOT be used! */
table.wide-data th,
table.data th,
#content table.data td.tableHeader
{
	color: #333;
	font-weight: bold;
	background-color: #dddddd;
    text-align: left;
}
#content table.data td.tableHeader{
	height:24px;
}

.fakelink {
  float:            left;
  cursor:           pointer;
  text-decoration:  underline;
  color:            #444 !important;
}
.fakelink:hover {
  color:            #000 !important;
}
.fakelink_last {
  cursor:           pointer;
  text-decoration:  underline;
  color:            #000 !important;
}

.reprint_button {
    display: block;
    border: 2px outset black;
    margin: 5px;
    padding: 2px 2px 2px 23px;
    text-decoration: none;
    background: url(/images/stock-print.png) 3px 4px no-repeat;
}

/*********************************************
 * Page/Section specific Style
 *********************************************/

table.customer_credit_log tr>th
{ width: 20% }
table.customer_credit_log tr>th+th
{ width: 25% }
table.customer_credit_log tr>th+th+th
{ width: 25% }
table.customer_credit_log tr>th+th+th+th
{ width: 10% }
table.customer_credit_log tr>th+th+th+th+th
{ width: 10% }
table.customer_credit_log tr>th+th+th+th+th+th
{ width: 10% }

div.qc_reason {
    margin-top: 15px;
}
table .packing_qc {
    width: 21%;
}

.marketing_promotion_packer_msg SPAN {
    background:         url(/images/icons/information.png) #EEE 10px 7px no-repeat;
    display:            block;
    line-height:        1.7em;
    padding:            5px 5px 5px 30px;
    margin-bottom:      0 0 10px 0;
    color:              #000;
    font-weight:        normal;
    border:             1px dashed #999;
}

/* Classes used on Putaway problem resolution page */
.putaway-problem-resolution-error-date {
    font-style: italic
}

.putaway-problem-resolution-re-putaway-block {
    border: 1px dotted #999999;
    padding: 1.5em;
}

/*********************************************
 * Overrides
 *********************************************/

/* handheld element overrides */

.handheld input,
.handheld input[type="button"],
.handheld input[type="submit"] {
    width: auto;
    min-width: 0;
}

/* handheld layout overrides */

.handheld #container {
	width: 210px;
	height: 100%;
	margin-left: 3px;
}

.handheld #header {
	width:210px;
	padding-bottom: 3px;
	border-bottom: 7px solid #494949;
}

.handheld #header img {
    display: inline;
    vertical-align: -2px;
}

.handheld #header span {
    color: #999;
    border-right: 1px solid #CCC;
    padding: 0 7px;
    font-weight: bold;
    font-size: 10px;
    margin:0;
}
.handheld #header span.dc {
    color: #333;
    border: 0;
    padding: 0 0 0 7px;
}

.handheld #content {
    margin: 0;
    padding-bottom: 20px;
    border-bottom: 12px solid #494949;
}

.handheld #contentLeftCol {
    float: none;
    width: auto;
    text-align: left;
	color: #666;
	border-bottom: 1px dotted #bbb;
}
.handheld #contentLeftCol li {
    display: inline;
    margin: 0;
    padding: 0 5px;
    border-right: 1px solid #CCC;
}
.handheld #contentLeftCol li a {
    border-bottom: 0;
}

.handheld #channelTitle {
    position: relative;
    top: 0;
    left: 0;
    margin: 5px 0;
}

.handheld #contentRightCol {
    float: none;
	padding: 0px;
	margin: 10px 0 0px 0px;
	padding-left: 0px;
}



/* other handheld overrides */

.handheld ul.homemenu li {
    list-style-image:     none;
    list-style-type:      none;
    margin: 7px 0;
    font-weight: bold;
    font-size: 1.1em;
}
.handheld ul.homemenu li.isaction {
    list-style-type:      disc;
    margin-top: 20px;
}
.handheld ul.homemenu li ul li {
    list-style-type:      disc;
    font-weight: normal;
    margin-left: 20px;
    font-size: 0.9em;
}


.handheld .narrowlabels .formrow {
    padding: 3px 0;
}
.handheld .narrowlabels .formrow label,
.handheld .narrowlabels .formrow .fakelabel {
    margin-left: 0;
    padding-right: 10px;
    width: 70px;
}
.handheld .narrowlabels .formrow input {
    width: 80px;
}

.handheld .narrowlabels .formrow.buttons {
    padding-left: 80px;
}
.handheld .narrowlabels .formrow.buttons input {
    width: auto;
}


#barcode_scan {
    margin: 15px 0;
}


/*********************************************
 * leftovers - still need sorting
 *********************************************/



/* form classes */

#content input,
#content select,
#content textarea {
	color: #333;
	font-weight: normal;
	font-size: 1em;
}

#content input.xlarge,
#content textarea.xlarge {
	font-size: 1.2em;
}


#content input.button, #content button.button {
	padding-left: 4px;
	padding-right: 3px;
	padding-top: 3px;
	padding-bottom: 3px;
	color: #fff;
	font-weight: bold;
	font-size: 0.9em;
	border:0px;
	background-color: #494949;
    cursor: pointer;
}

#content input.enormousbutton {
    font-size: 1.5em;
    background: green;
}

#content table td {
	color: #333;
	font-weight: normal;
}

#content table td.divider {
	background-image: url("/images/dots.gif");
	height:1px;
}

#content table.data tr {
	color: #333;
	font-weight: normal;
	background-color: #f2f2f2;
}

#content table.tablesorter thead th {
    border-top: 1px solid #aaaaaa;
    border-bottom: 1px solid #aaaaaa;
}

#content table.tablesorter thead th.followingValueColumn {
    display: none;
    /* background: #ff0000; */
}

#content table.tablesorter thead th.sortable {
    cursor: pointer;

    padding-right: 12px;
    background-image: url("/images/tablesorter/bg.gif");
    background-position: right center;
    background-repeat: no-repeat;
}

#content table.tablesorter thead th.sortable.headerSortUp {
    background-image: url("/images/tablesorter/asc.gif");
}

#content table.tablesorter thead th.sortable.headerSortDown {
    background-image: url("/images/tablesorter/desc.gif");
}

#content table.tablesorter td {
    border-bottom: 1px solid #bbbbbb;
}
/* Disable bottom borders in nested tables */
#content table.tablesorter table td {
    border-bottom: none;
}
#content table.tablesorter td.followingValueColumn {
    display: none;
    /* background: #ff0000; */
}

#content table.data td {
	height:24px;
}
#content table.wide-data td {
    /*
       need this to overide the above
       while we still need to maintain
       backward compatibility
    */
    height:auto !important;
}

#content table.data td.light {
	color: #999;
}
#content table.data td.light a {
	color: #999;
	text-decoration: underline;
}

#content table.data td.light a:hover {
	text-decoration: none;
}

#content table.data td.light .highlight {
	color: #999;
}

#content table.data td.divider {
	background-image: url("/images/dots.gif");
	height:1px;
}

#content table.data td.dividerHeader {
	background-image: url("/images/dots.gif");
	background-color:#e5e5e5;
	height:1px;
}

#content table.data td.tableHeader {
	color: #333;
	font-weight: bold;
	background-color: #dddddd;
	height:24px;
}

#content table.data td.tableHeader .highlight {
    color: #000;
	font-weight: bold;
}

#content table.data td.blank {
	background-color:#FFF;
	height:1px;
}

#content table.data tr.totals td {
    background-color:#FFF;
    font-weight: bold;
}

#content table.data td.numbers, #content table.data th.numbers {
    text-align: right;
    padding-right: 1em;
}

#content table.data tr.expand td {
    border: none;
}

#content table.data td.spacer {
         background-color:#FFF;
         height:20px;
}

#content table.data td.none {
         background-color:#FFF;
         height:0px;
}

#content table.data td.warn, #content table.data tr.warn, #content table.data tr.warn td {
         background-color:#F0B9B9;
}

#content table.data td.refund_warn {
         background-color:#808080;
}

#content table.data td.highlight {
         background-color: #CCFFCC;
}

#content table.data tr.highlight {
         background-color: #CCFFCC;
}

#content table.data tr.highlight td {
         background-color: #CCFFCC;
}

#content table.data td.highlight2 {
         background-color: #DDA0DD;
}

#content table.data tr.highlight2 {
         background-color: #DDA0DD;
}

#content table.data tr.highlight2 td {
         background-color: #DDA0DD;
}

#content table.data td.highlight3 {
