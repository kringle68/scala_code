<AJAXAuthenticationCallerURL>
    url  /Finance/FraudRules/Staging
</AJAXAuthenticationCallerURL>

<AJAXLoginRefererURL>
    url  /Finance/FraudRules/ViewStagingRules
</AJAXLoginRefererURL>

<NAP::CustomerCredit::Client>
  [% IF LOCAL_DEVEL %]
  user_agent_class NAP::CustomerCredit::TestUserAgent
  test_data_dir [% XTDC_BASE_DIR %]/t/tmp/custcred
  [% END %]
  api_uri [% CUSTOMERCREDIT_API_URL %]
</NAP::CustomerCredit::Client>

<GiftMessageImageGenerator>
    NAP    [% GIFT_MESSAGE_GENERATOR_URL_NAP %]
    MRP    [% GIFT_MESSAGE_GENERATOR_URL_MRP %]
</GiftMessageImageGenerator>

<PaymentService>
    service_url [% PAYMENT_SERVICE_URL %]
    payment_form_url [% PAYMENT_SERVICE_PAYMENT_FORM_URL %]
    basic_auth_username [% PAYMENT_SERVICE_BASIC_AUTH_USERNAME %]
    basic_auth_password [% PAYMENT_SERVICE_BASIC_AUTH_PASSWORD %]
</PaymentService>

<DHLDeliverySFTPServer>
    server      ftp3.dhl.com
    username    porter
    password    [% DHL_DELIVERY_SFTP_PASSWORD %]
    debug       0
    directory   out/work
</DHLDeliverySFTPServer>

<DHL_FFSTATFile>
    max_failures 5
</DHL_FFSTATFile>

<PreOrder>
    default_summary_interval  6 month
    used_summary_interval   6 month
</PreOrder>

<ACL>
    <role_parsing>
        role_prefix                     app_
        role_separator                  ~
        property_separator              &
        property_key_value_separator    .
    </role_parsing>

    # enable some paths through the system (mainly API calls in the Catalyst
    # APP) that do not require the same level of authentication as normal pages
    <insecure_paths>
        path    api
        path    pricing/prices_for_all_countries
        path    pricing/reload_cache
        path    sizing/sizes_for_product
        path    truckdepartures
        path    metrics
    </insecure_paths>
</ACL>
# include the following file which should be temporary until the end
# of the XT Access Controls project and therefore easy to remove
<<Include xtracker_acl_interim.conf>>

<RunningEnvironment>
[% IF DEV_ENV || NAP_ENV == 'DAVE(genesis)' %]dave    Y
[% ELSE %]live    Y[% END %]
</RunningEnvironment>

[%- # the "default" means that, if AUTO_INVALIDATE_PRICING_CACHE is
    # not set anywhere, we behave as if it were true; so we don't have
    # to add it everywhere, just to cater to the special XTs that may
    # need it set to false

   DEFAULT AUTO_INVALIDATE_PRICING_CACHE='true';
   IF AUTO_INVALIDATE_PRICING_CACHE != 'true'; %]
<Controller::Pricing>
 auto_invalidate_cache 0
</Controller::Pricing>
[%- END %]

<PrinterSetupScript>
    enabled [% ENABLE_PRINTER_SETUP_SCRIPT %]
</PrinterSetupScript>

<TestTools>
    enable_side_panel [% ENABLE_SIDE_PANEL %]
</TestTools>

<Pims>
    js_url [% IF DEV_ENV %]/test/pims[% ELSE %]/pims[% END %]
    api_class [% IF DEV_ENV %]NAP::Pims::API::Mocked[% ELSE %]NAP::Pims::API[% END %]
    url [% PIMS_URL %]
</Pims>