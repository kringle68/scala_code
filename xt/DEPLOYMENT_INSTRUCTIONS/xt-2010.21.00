        WEB_LIVE_MRP_INTL_DB_USER
        WEB_LIVE_MRP_INTL_DB_PASS
        WEB_LIVE_MRP_INTL_DB_TYPE
        WEB_LIVE_MRP_AM_DB_NAME
        WEB_LIVE_MRP_AM_DB_HOST
        WEB_LIVE_MRP_AM_DB_USER
        WEB_LIVE_MRP_AM_DB_PASS
        WEB_LIVE_MRP_AM_DB_TYPE
        WEB_STAGING_MRP_INTL_DB_NAME
        WEB_STAGING_MRP_INTL_DB_HOST
        WEB_STAGING_MRP_INTL_DB_USER
        WEB_STAGING_MRP_INTL_DB_PASS
        WEB_STAGING_MRP_INTL_DB_TYPE
        WEB_STAGING_MRP_AM_DB_NAME
        WEB_STAGING_MRP_AM_DB_HOST
        WEB_STAGING_MRP_AM_DB_USER
        WEB_STAGING_MRP_AM_DB_PASS
        WEB_STAGING_MRP_AM_DB_TYPE

